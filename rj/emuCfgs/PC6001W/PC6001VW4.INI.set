stop_screen_saver=off	;Prohibit the screen saver (on:enable, off:disable)
pos_x=0				;Window positon
pos_y=0				;
drop_confirm=off		;Show a confirmation dialog after the drag && drop. (on:enable, off:disable)
screen_margin=4			;Window Margin (0-8)
screen_magni=100		;Window magnification (25-300 %)
screen_brightness=100	;Window brightness (0-500 %)
key_win=1				;Keyboard layout (0:DOS, 1:Windows)
trouble=0				;Trouble shooting flag
debug_log=0				;Debug log
						;  (1:FILE-IO, 2:SUB-CPU, 4:INTR, 8:SND, 16:TIMER, 32:I/O)
screenshot_format=0		;Screenshot image type (0:BMP, 1:JPG, 2:PNG, 3:GIF)
