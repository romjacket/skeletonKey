baudrate=1000			;Tape baudrate (600-1200 baud)
roman_kana=on			;Kana/Roman input (on:enable, off:disable)
jimaku=1				;Subtitles (0:enable, 1:disable)
overdrive=2				;Fixed speed magnification (0:30%, 1:60%, 2-9:x100%)
device=0				;device
